<div class="category-page-card col">
  <div class="img-holder">
    <% if taxon.icon.present? %>
      <% image_link = image_cdn_link(taxon.icon.url(:medium))%>
      <%= link_to (image_tag image_link, alt: '', loading: 'lazy'), seo_url(taxon), class: 'clear-link' %>
    <% end %>
  </div>
  <p class="sub-title"><%= link_to taxon.name, seo_url(taxon), class: 'clear-link' %></p>
</div>

